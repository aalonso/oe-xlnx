From 39607fc9b5afafbab0292d9282b4a675524cf901 Mon Sep 17 00:00:00 2001
From: aalonso <aalonso00@gmail.com>
Date: Wed, 30 Dec 2009 23:35:38 -0600
Subject: [PATCH] ac prereq downgrade

---
 config/override.m4     |    6 +++---
 configure.ac           |    2 +-
 intl/configure.ac      |    2 +-
 libiberty/configure.ac |    2 +-
 4 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/config/override.m4 b/config/override.m4
index cfa225a..56c6b92 100644
--- a/config/override.m4
+++ b/config/override.m4
@@ -40,7 +40,7 @@ m4_copy_force([_AC_PREREQ], [AC_PREREQ])
 
 dnl Ensure exactly this Autoconf version is used
 m4_ifndef([_GCC_AUTOCONF_VERSION],
-  [m4_define([_GCC_AUTOCONF_VERSION], [2.64])])
+  [m4_define([_GCC_AUTOCONF_VERSION], [2.63])])
 
 dnl Test for the exact version when AC_INIT is expanded.
 dnl This allows to update the tree in steps (for testing)
@@ -64,9 +64,9 @@ m4_ifndef([AC_DISABLE_OPTION_CHECKING],
   [m4_define([AC_DISABLE_OPTION_CHECKING])])
 
 
-dnl Fix 2.64 cross compile detection for AVR and RTEMS
+dnl Fix 2.63 cross compile detection for AVR and RTEMS
 dnl by not trying to compile fopen.
-m4_if(m4_defn([m4_PACKAGE_VERSION]), [2.64],
+m4_if(m4_defn([m4_PACKAGE_VERSION]), [2.63],
   [m4_foreach([_GCC_LANG], [C, C++, Fortran, Fortran 77],
      [m4_define([_AC_LANG_IO_PROGRAM(]_GCC_LANG[)], m4_defn([AC_LANG_PROGRAM(]_GCC_LANG[)]))])])
 
diff --git a/configure.ac b/configure.ac
index 1454f86..17cd19a 100644
--- a/configure.ac
+++ b/configure.ac
@@ -24,7 +24,7 @@ m4_include(config/override.m4)
 m4_include(config/proginstall.m4)
 
 AC_INIT(move-if-change)
-AC_PREREQ(2.64)
+AC_PREREQ(2.63)
 AC_DISABLE_OPTION_CHECKING
 
 progname=$0
diff --git a/intl/configure.ac b/intl/configure.ac
index 698c330..86f4154 100644
--- a/intl/configure.ac
+++ b/intl/configure.ac
@@ -1,4 +1,4 @@
-AC_PREREQ(2.64)
+AC_PREREQ(2.63)
 AC_INIT
 AC_CONFIG_SRCDIR(gettext.c)
 AC_CONFIG_HEADER(config.h)
diff --git a/libiberty/configure.ac b/libiberty/configure.ac
index c34fc10..d5db879 100644
--- a/libiberty/configure.ac
+++ b/libiberty/configure.ac
@@ -1,6 +1,6 @@
 dnl Process this file with autoconf to produce a configure script
 
-AC_PREREQ(2.64)
+AC_PREREQ(2.63)
 AC_INIT
 AC_CONFIG_SRCDIR([xmalloc.c])
 
-- 
1.6.5.2

